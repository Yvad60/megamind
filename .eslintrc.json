{
	"env": {
		"browser": true,
		"es2021": true
	},
	"settings": {
		"import/resolver": {
			"node": {
				"extensions": [".js", ".jsx", ".ts", ".tsx"]
			}
		}
	},
	"extends": ["airbnb-base", "airbnb-typescript/base", "prettier"],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module",
		"project": "tsconfig.json"
	},
	"plugins": ["@typescript-eslint"],
	"rules": {
		"import/prefer-default-export": 0
	}
}
